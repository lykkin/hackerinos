=provide(:title, "Posterino a hackerino")
%div#submitForm
    =form_for(@hackerino) do |f|
        =f.label :title
        =f.text_field :title

        =f.label :url
        =f.text_field :url

        =f.label :author
        =f.text_field :author

        =f.submit "Posterino this hackerino", class: "button"
